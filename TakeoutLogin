public static TakeoutLogin(String adminName,String password){
    try{
	String sql="select * from ' users' where adminName=?";
                ResultSet re=psql.executeQuery(sql);
       
                if (re.next()){
                     return 1;
                }
                else {
                    return 0;
                 }
           }
          catch (SQLException e){
                e.printStackTrace();
                System.out.println("数据库连接失败");
            }
         
        return 0;
      }

